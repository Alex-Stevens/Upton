{
	"manifest_version": 2,

	"name": "Opera Extensions Getting Started",
	"description": "Sample extension",
	"version": "1.0",
	"background": {
		"scripts": ["background.js"]
	},

	"permissions": [ "tabs", "*://*/*" ],
	"browser_action": {
		"default_icon": "icon.png",
		"default_title": "Send to service"
	}
}